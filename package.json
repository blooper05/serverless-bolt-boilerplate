  "author": "blooper05",
  "engines": {
    "node": "12.18.0"
  },
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "tsc --project tsconfig.json",
    "clean": "rimraf dist",
    "predeploy": "yarn run build",
    "deploy": "serverless deploy",
    "deploy:dev": "yarn run deploy --stage dev",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "start:dev": "nodemon --config .nodemon.json --legacy-watch"
  },
  "dependencies": {
    "@slack/bolt": "^2.1.1",
    "@speee-js/jsx-slack": "^2.2.0",
    "aws-serverless-express": "^3.3.8"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.53",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/node": "^14.0.12",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "serverless": "^1.72.0",
    "serverless-deployment-bucket": "^1.1.2",
    "serverless-domain-manager": "^4.1.1",
    "serverless-layers": "^2.2.4",
    "serverless-offline": "^6.4.0",
    "serverless-prune-plugin": "^1.4.3",
    "ts-node": "^8.10.2",
    "typescript": "~3.7.5"
  }
